{
  "scripts": {
    "start": "npm-run-all --parallel start-server   start-client",
    "update-schema": "babel-node server/scripts/updateSchema.js",
    "clean-client": "find . -name __generated__ | xargs rm -rf",
    "compile-client": "npm run clean-client &&relay-compiler --src ./client --schema ./server/data/schema.graphql  --extensions js jsx tsx",
    "debug-compile": "node --debug-brk --inspect ./node_modules/relay-compiler/bin/relay-compiler --src ./js/ --schema ./data/schema.graphql --extensions js jsx tsx",
    "compile-client:watch": "npm run compile-client -- --watch",
    "build-client": "npm run compile-client&& export NODE_ENV=production&& webpack --config=client/webpack.config --progress --verbose --colors --display-error-details",
    "start-client": "npm run compile-client&& webpack-dev-server --config=client/webpack.config",
    "start-server": "nodemon ./server/index.js",
    "start-server-v2": "nodemon ./server.js  --exec babel-node  --watch data --ignore  data/schema.json --ignore data/schema.graphql",
    "debug-server": "babel-node ./server.js --source-maps --debug-brk --inspect",
    "debug-server-v2": "node --inspect-brk  ./node_modules/babel-cli/bin/babel-node.js  ./server.js",
    "test": "mocha --compilers js:babel-register --recursive \"component/**/*.spec.js\" --require test/setup.js",
    "debug-mocha": "mocha --compilers js:babel-register  --recursive \"js/**/*.spec.js\" --require test/setup.js --debug-brk --inspect",
    "lint": "eslint ./js",
    "start-heroku": "node heroku/index.js",
    "heroku-postbuild": "npm run build-client"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.17.2",
    "casual": "^1.5.17",
    "eventemitter2": "^4.1.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.4",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "graphql-relay-subscription": "^0.1.3",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "casual": "^1.5.17",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "eslint": "3.19.0",
    "eslint-config-fbjs": "1.1.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.15.0",
    "eslint-plugin-react": "5.2.2",
    "expect": "^1.20.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.57.3",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "prop-types": "^15.6",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-relay": "^1.6.0",
    "relay-compiler": "^1.6.0",
    "relay-runtime": "^1.6.0",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.21.0",
    "todomvc-app-css": "^2.1.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "whatwg-fetch": "^2.0.4"
  }
}
